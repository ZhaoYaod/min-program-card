.seachPoint{
  width:70.2rpx;
  height: 70.2rpx;
  border-radius: 50%;
  font-size: 70.2rpx;
  animation: headRotate 1s linear infinite;
  /* margin: 0 auto; */
  position: absolute;
  left: 46%;
  top:46%;
  transform: translate(-50%,-50%);
}
.addAnimation{
  animation: headRotate 6s linear infinite
}
/* 头像旋转效果  */
@keyframes headRotate{
  0% {transform: rotate(0deg);}
  50% {transform: rotate(180deg);}
  100% {transform: rotate(360deg);}
}