.shade{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 5;
  background-color: rgba(0, 0, 0, 0)
}
.functabshow{
  background-color: #f2f2f2!important;
}
.mask {
  padding-left: 4.68rpx;
  padding-right: 4.68rpx;
  width: 100%;
  box-sizing: border-box;
  height: 100%;
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 5;
}
.scan-box {
  background-color: white;
  height: 629.46rpx;
  position: fixed;
  bottom: 0;
  left:4.68rpx;
  right:4.68rpx;
  border-top-left-radius: 14.04rpx;
  border-top-right-radius: 14.04rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 32.76rpx;
  color:#797979;
  text-align: left;
  box-sizing: border-box;
  padding-top:30rpx;
  z-index: 9;
  animation: sflash 0.2s linear both;
}
@keyframes sflash {
  from {
    height: 0;
    bottom: -629.46rpx;
  }

  to {
    height: 629.46rpx;
    bottom: 0rpx;
  }
}
.scan-box button{
  font-size: 32.76rpx;
  color: #797979;
  text-align: left;
  width: 100%;
  padding-left: 53.82rpx;
  padding-bottom: 20rpx;
  box-sizing: border-box;
  background-color: white;
}
.scan-box button::after{
  border: 0;
}
