.seachPoint{
  position: relative;
  top:40vh;
  width:70.2rpx;
  height: 70.2rpx;
  margin: 0 auto;
  border-radius: 50%;
  color: rebeccapurple;
  animation: headRotate 1s linear infinite;
  display: block;
}
@keyframes headRotate{
  0% {transform: rotate(0deg);}
  50% {transform: rotate(180deg);}
  100% {transform: rotate(360deg);}
}
.numbtn{
  background-color: rgba(0,0, 0, 0);
}
.numbtn::after{
  border: 0;
}



