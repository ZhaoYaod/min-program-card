/* pages/peoplenearby/peoplenearby.wxss */
.personal{
  height:138.06rpx;
  position: relative;
}
.personalSelected{
  background:#F2F2F2;
}
.personal-box{
  width:678.6rpx;
  height:138.06rpx;
  border-bottom:4.68rpx solid #F8FAFB;
  margin-left:37.44rpx;
}
/* 头像 */
.personal-img{
  width:93.6rpx;
  height:93.6rpx;
  position: absolute;
  top:25.74rpx;
  border-radius:9.36rpx;
}
/* 姓名 */
.personal-name{
  margin-left:119.34rpx;
  color:#122F46;
  font-size:28.08rpx;
  padding-top:25.74rpx;
}
/* 是否认证 */
.personal-certified,.personal-certified1{
  position: absolute;
  right:168.48rpx;
  top:32.76rpx;
  font-size:23.4rpx;
  display: inline-block;
  width:84.24rpx;
  height:37.44rpx;
  border-radius:4.68rpx;
  text-align: center;
  line-height: 37.44rpx;
}
/* 已认证 */
.personal-certified{
  /* background:#FF0066; */
  background:#F22C2C;
  color: #fff;
}
/* 未认证 */
.personal-certified1{
  background:#E4E4E4;
  color:#BCBCBC;
}
/* 距离 */
.personal-distance{
  position: absolute;
  right:37.44rpx;
  color:#A1A1A1;
  font-size:28.08rpx;
  top:32.76rpx;
}
/* 简介 */
.personal-introduction{
  margin-left:119.34rpx;
  font-size:28.08rpx;
  color:#A1A1A1;
  padding-top:16.38rpx;
  width:552.24rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
/* 转发 */
.forward-box{
  position:absolute;
  left:238.68rpx;
  top:70.2rpx;
  width:231.66rpx;
  height:81.9rpx;
  border-radius:4.68rpx;
  box-shadow:0 0 7.02rpx #797979;
  background:#fff;
  color:#122F46;
  font-size:30.42rpx;
  line-height: 81.9rpx;
  margin:0;
  padding:0;
  text-align: left;
  text-indent: 28.08rpx;
}
.forward-box-bg{
  background:#f2f2f2;
}
.forward-box::after{
  border:0;
}
.loading-more{
  position:absolute;
  bottom:-138.06rpx;
  width:100%;
  height:117rpx;
  font-size:12px;
  color:#ccc;
  text-align:center;
  line-height:117rpx;
  display:flex;
  justify-content:center;
  /*x轴对齐方式*/
  align-items:center;
}
.list-box{
  position: relative;
}
::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}
.loading-more-img {
  margin-right:11.7rpx;
  width: 35.72rpx;
  height: 35.72rpx;
  /* transform-origin: 50% 50%; */
  animation: headRotate 2.5s linear infinite;
  /* position:absolute;
  left:219.44rpx;
  top:calc(50% - 17.86rpx); */
  background-image:url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size:cover;

}
@keyframes headRotate {
  0% {
    transform: rotate(0)
  }
  50% {
    transform: rotate(180deg)
  }

  100% {
    transform: rotate(360deg)
  }
}
.operatSelected {
  color: #1081FF !important;
}