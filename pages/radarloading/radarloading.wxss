/* pages/radarloading/radarloading.wxss */
page{
  height: 100%;
  position: fixed;
}
.bg{
  height:100%;
  width: 100%;
  position: relative;
  left:0;
  top:0;
  background-size:100%;
}
.circle-box{
  width:748.8rpx;
  height:826.02rpx;
  position:relative;
  top:93.6rpx;
}
.quan{
  width:100%;
  height:100%;
}
.yuan {
  position: absolute;
  left: calc(50% - 23.4rpx);
  top: calc(50% - 23.4rpx);
  width: 46.8rpx;
  height: 46.8rpx;
  z-index: 1;
}
.zhuan{
  position: absolute;
  left:91.26rpx;
  top:0rpx;
  width:283.14rpx;
  height:413.01rpx;
  transform-origin: bottom right;
  animation: headRotate 3.5s linear infinite;
  opacity: 0.3;
}
@keyframes headRotate{
  0% {transform: rotate(0)}
  50% {transform: rotate(180deg)}
  100%{transform: rotate(360deg)}
}
.photo{
  position:absolute;
  border-radius:50%;
  animation: breathLamp 1.8s alternate infinite;
}
@keyframes breathLamp {
 0% {
    transform:translateY(4.68rpx);
    box-shadow: 0 0 4.68rpx #fff; 
  }

  100% {
    transform:translateY(-4.68rpx);
    box-shadow: 0 0 23.4rpx #ffffff21;
  }
}

