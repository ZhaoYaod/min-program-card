/* 三条杠动画 */
@keyframes loading {
  0% {
    transform: translateY(4.68rpx);
  }

  100% {
    transform: translateY(-4.68rpx);
  }
}
.button-text{
  color:#21f0f3;
  font-size:32.76rpx;
  display:block;
  width:100%;
  height:100%;
  text-align:center;
  line-height:86.58rpx;
}
.serach-no-text{
  font-size:28.08rpx;
  color:#21f0f3;
}
/* 按钮动画(无数据) */
.animation-button-n{
    animation: buttonLoad 2.8s 1;
   
}
 @keyframes buttonLoad{
   0%{
     transform:translateY(364rpx);
   }
   100%{
     transform:translateY(0);
   }
 }
 /* 按钮动画(有数据) */
 .animation-button-y {
   animation: buttonLoad 1.8s 1;

 }

 @keyframes buttonLoad {
   0% {
     transform: translateY(264rpx);
   }

   100% {
     transform: translateY(0);
   }
 }
 /* 提示文字动画 */
 .animation-text {
   animation: textLoad .6s 1;
 }

 @keyframes textLoad {
   0% {
     transform: translateY(34rpx);
   }

   100% {
     transform: translateY(0);
   }
 }

